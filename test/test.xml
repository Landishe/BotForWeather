    <test-case id="weatherNow - day">
        <dateTime>2026-02-02 15:30:00</dateTime>
        <mockData>
            <query method="get">https://api.open-meteo.com/v1/forecast?latitude=${latitude}&amp;longitude=${longitude}&amp;timezone=${timezone}&amp;current=${current}&amp;daily=${daily}</query>
            <parameters>
                <latitude>55.75222</latitude>
                <longitude>37.61556</longitude>
                <current>temperature_2m,weather_code,wind_speed_10m</current>
                <timezone>auto</timezone>
                <daily>temperature_2m_max,weather_code,wind_speed_10m_max,sunset,sunrise</daily>
            </parameters>
            <response type="json" status="200">
                {
                    "latitude": 55.75,
                    "longitude": 37.625,
                    "current": {
                        "time": "2026-02-02T15:30",
                        "temperature_2m": -12.3,
                        "weather_code": 45,
                        "wind_speed_10m": 5.2
                    },
                    "daily": {
                        "temperature_2m_max": [8.7],
                        "weather_code": [3],
                        "wind_speed_10m_max": [2.3],
                        "sunrise": ["2026-02-02T07:44"],
                        "sunset": ["2026-02-02T16:41"]
                    }
                }
            </response>
        </mockData>
        <q state="/start">/start</q>
        <a>** –û—Ç —Ä–∞–∑—Ä–∞–±–æ—Ç—á–∏–∫–∞: –ë–æ—Ç –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –≤ —Å—Ç–∞–¥–∏–∏ —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∏ –∏ –º–æ–≥—É—Ç –±—ã—Ç—å –æ—à–±–∫–∏. –ë–æ—Ç —Ä–∞–∑–±–∏—Ä–∞–µ—Ç —Å–æ–∫—Ä–∞—â–µ–Ω–Ω—ã–µ –Ω–∞–∑–≤–∞–Ω–∏–µ –≥–æ—Ä–æ–¥–æ–≤ –Ω–∞–ø—Ä–∏–º–µ—Ä: –°–ü–ë, –ú–°–ö –ï–∫–± –∏ —Ç–ø. –î–æ–±–∞–≤–ª–µ–Ω–æ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –≤—ã–≤–æ–¥–∞ –ø–æ–≥–æ–¥—ã –Ω–∞ "—Å–µ–π—á–∞—Å"** –î–æ–±—Ä—ã–π –¥–µ–Ω—å! –Ø –º–æ–≥—É –ø–æ–¥—Å–∫–∞–∑–∞—Ç—å –ø—Ä–æ–≥–Ω–æ–∑ –ø–æ–≥–æ–¥—ã –≤ –≤–∞—à–µ–º –≥–æ—Ä–æ–¥–µ.</a>
        <a state="/start/whereAreYou">–£—Ç–æ—á–Ω–∏—Ç–µ –≤ –∫–∞–∫–æ–º –≥–æ—Ä–æ–¥–µ –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å –ø–æ–≥–æ–¥—É?</a>
